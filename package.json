{
  "name": "goodreads-api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "server": "nodemon -e js ./bin/www --ignore tests",
    "client": "npm run start:app --prefix client",
    "dev": "npm-run-all --parallel server client",
    "test": "jest -- tests",
    "test:cov": "jest --coverage -- tests",
    "bootstrap": "npm install --prefix client && npm run bootstrap --prefix client && npm run build:components --prefix client",
    "rebuild": "npm run build:components --prefix client",
    "clean:mongo": "rm -rf seed/mongo-volume"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "mongoose": "^5.11.13",
    "morgan": "~1.9.1",
    "passport": "^0.4.1",
    "passport-github2": "^0.1.12",
    "passport-local": "^1.0.0",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "axios": "^0.21.1",
    "chai": "^4.2.0",
    "jest": "^26.6.3",
    "mocha": "^8.2.1",
    "moxios": "^0.4.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "sinon": "^9.2.4",
    "supertest": "^6.1.3"
  }
}
